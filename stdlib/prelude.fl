(?T enum: T some) .\some
(   enum none ()) .\none

{
    some: some,
    none: none,

    map: ?T \e: enum { some: T, none: () } ?U \f: T -> U
        e.match {
            some \t:T some (f t),
            none \():() none,
        }
    ,

    flip: ?A ?B ?C \f: A -> B -> C
        \b:B \a:A f a b
    ,
}
